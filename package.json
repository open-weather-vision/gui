	"scripts": {
		"update:routes": "node util/updateRoutes.js",
		"web:dev": "env-cmd -f .env.development.web concurrently --kill-others \"less-watch-compiler --config less-watcher.config.json\" \"node util/updateRoutesWatch.js\" \"react-scripts start\"",
		"web:build": "env-cmd -f .env.production.web node util/updateRoutes.js && react-scripts build",
		"web:start": "env-cmd -f .env.production.web serve -s build",
		"electron:dev": "env-cmd -f .env.development.electron concurrently --kill-others \"less-watch-compiler --config less-watcher.config.json\" \"node util/updateRoutesWatch.js\" \"react-scripts start\" \"npx electron .\"",
		"electron:build": "env-cmd -f .env.production.electron node util/updateRoutes.js && react-scripts build",
		"electron:start": "env-cmd -f .env.production.electron npx electron ."
